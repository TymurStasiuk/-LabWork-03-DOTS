
def main():
    n = int(input())
    adj = [list(map(int, input().split())) for _ in range(n)]
    colors = list(map(int, input().split()))
    
    count = 0
    for i in range(n):
        for j in range(i + 1, n):  
            if adj[i][j] == 1 and colors[i] != colors[j]:
                count += 1
                
    print(count)

if __name__ == "__main__":
    main()
